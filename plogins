curl -XPOST -H "Content-Type: application/json" -d '{ "username": "randomuser", "name": "random", "email": "random@random.com", "password": "random1234" }' http://localhost:3000/users

curl -XPOST -i -H "Content-Type: application/json" -d '{ "email": "random@random.com", "password": "random1234" }' http://localhost:3000/auth/login 
